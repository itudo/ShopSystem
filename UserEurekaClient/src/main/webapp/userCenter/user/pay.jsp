<script>
$(function() {
	var href = window.location.href;
	var params = href.substring(href.indexOf("?")+1,href.length).split("&");
	var id = params[0].substring(params[0].indexOf("=")+1,params[0].length);
	var money = params[1].substring(params[1].indexOf("=")+1,params[1].length);
	$("#detailid").html(id);
	$("#money").html(money);
	$(".Pay_now_apply_for").click(function() {
		$.getJSON("http://localhost:8762/service-user/pay.action",{order_id:id},function(data) {
			if(data.code==1) {
				toastr.error('恭喜您，支付成功');	
				window.location.href = "http://localhost:8762/service-user/userCenter/user/myOrder.jsp";
			}
		})
	})
})
</script>
